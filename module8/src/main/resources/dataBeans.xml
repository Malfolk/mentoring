<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.shop.SellerInitializer" autowire="byType">
        <constructor-arg>
            <map>
                <entry key="1" value="test"/>
                <entry key="2" value="test"/>
            </map>
        </constructor-arg>
    </bean>
    <bean class="org.shop.ProposalInitializer" autowire="byName"/>
    <bean class="org.shop.ProductInitializer">
        <constructor-arg ref="productService"/>
    </bean>
    <bean class="org.shop.DataInitializer" init-method="initData" autowire="byType"/>
    <bean id="userInitializer" class="org.shop.UserInitializer">
        <constructor-arg ref="userService"/>
    </bean>
</beans>